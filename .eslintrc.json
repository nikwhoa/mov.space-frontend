{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "react-app"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "github"],
  "rules": {
    "space-before-function-paren": "off",
    "react/react-in-jsx-scope": "off",
    "space-before-blocks": "off",
    "semi": "off",
    "no-trailing-spaces": "off"
  }
}
